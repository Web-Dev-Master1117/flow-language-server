[ignore]
; annotated with `@flow` but have errors
<PROJECT_ROOT>/node_modules/fbjs/lib/.*
<PROJECT_ROOT>/node_modules/.*/test/.*
<PROJECT_ROOT>/node_modules/nuclide-commons-atom/workspace-views-compat.js.flow
<PROJECT_ROOT>/atom-languageserver-flow/node_modules/.*/test/.*
<PROJECT_ROOT>/atom-languageserver-flow/node_modules/atom-languageclient

[include]

[libs]
flow-libs/

[options]
emoji=true
experimental.const_params=true
experimental.strict_type_args=true
module.use_strict=true
unsafe.enable_getters_and_setters=true
esproposal.export_star_as=enable

suppress_comment=.*\\$FlowFixMe.*
suppress_comment=.*\\$FlowIssue.*
suppress_comment=.*\\$FlowIgnore.*
suppress_comment=.*\\$FlowFB.*

[version]
0.50.0
