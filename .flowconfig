<PROJECT_ROOT>/node_modules/nuclide-commons/debounce.js.flow
<PROJECT_ROOT>/node_modules/nuclide-commons/nuclideUri.js.flow
<PROJECT_ROOT>/node_modules/nuclide-commons/string.js.flow
<PROJECT_ROOT>/node_modules/nuclide-commons-atom/ActiveEditorRegistry.js.flow
